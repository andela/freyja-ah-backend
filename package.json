{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:migrate && NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register --require babel-polyfill ./server/test/ --recursive --timeout 5000 --exit",
    "migrate": "sequelize db:migrate",
    "undo-all:migrate": "sequelize db:migrate:undo:all",
    "refreshdb": "npm run undo-all:migrate && npm run migrate && npm run commit-seed",
    "undo-current:migrate": "sequelize db:migrate:undo",
    "test:migrate": "NODE_ENV=test npm run undo-all:migrate && NODE_ENV=test npm run migrate && NODE_ENV=test npm run commit-seed",
    "start": "node --require babel-polyfill ./build/index.js",
    "start:dev": "nodemon --exec babel-node ./server/index.js",
    "clean": "rm -rf ./build && mkdir build",
    "build": "npm run clean && babel -d build ./server",
    "heroku-postbuild": "npm run build",
    "coverage": "nyc report --reporter=lcov --reporter=text-lcov | coveralls",
    "commit-seed": "sequelize db:seed:all"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.4.1"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.4.4",
    "@sendgrid/mail": "^6.4.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.14.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.16.3",
    "express-session": "^1.16.1",
    "express-validator": "^5.3.1",
    "faker": "^4.1.0",
    "jsdoc": "^3.6.2",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nock": "^10.0.6",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.2",
    "request": "^2.87.0",
    "sequelize": "^5.8.6",
    "sequelize-cli": "^5.4.0",
    "sequelize-test-helpers": "^1.1.2",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.5",
    "underscore": "^1.12.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/node": "^7.2.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "istanbul": "^0.4.5",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sinon-chai": "^3.3.0"
  }
}
